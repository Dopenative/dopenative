    "react native",
    "bootstrap",
    "nativebase",
    "expo",
    "component library",
    "tailwind",
    "material",
    "cupertino",
    "react native web",
    "theming",
    "react"
  ],
  "author": "Florian Marcu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dopenative/dopenative/issues"
  },
  "homepage": "https://dopenative.com",
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.15.11",
    "expo-localization": "11.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.9",
    "babel-loader": "^8.2.3",
    "eslint": "8.2.0",
    "i18n-js": "3.8.0",
    "jest": "^26.0.1",
    "jest-expo": "^40.0.1",
    "prettier": "2.4.1",
    "react": "17.0.1",
    "react-native": "0.64.3",
    "react-native-builder-bob": "^0.18.2",
    "rollup": "^2.34.1",
    "rollup-plugin-flow": "^1.1.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "files": [
    "src",
    "lib"
